{
  "name": "pr-bot",
  "version": "0.0.1",
  "description": "A basic bot to fetch PRs",
  "main": "dist/index.js",
  "author": "Tim Austin <tim@neenjaw.com> (https://neenjaw.com)",
  "scripts": {
    "build": "npm run clean && tsc --build src",
    "clean": "rimraf dist",
    "serve": "functions-framework --source=dist/ --target=prBot",
    "start": "npm run build && npm run serve",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@slack/bolt": "^3.3.0",
    "dotenv": "^10.0.0",
    "octokit": "^1.0.5"
  },
  "engines": {
    "node": "14.15.2"
  },
  "repository": {
    "url": "https://github.com/neenjaw/prbot"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "slack",
    "octokit",
    "typescript"
  ],
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/express": "^4.17.12",
    "@types/node": "^15.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "files": [
    "dist"
  ]
}
